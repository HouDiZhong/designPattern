{
    "presets": [
       ["env", {
          "modules": false,
          "targets": {
             "node": "current",
             "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
          }
       }]
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": true,
            "polyfill": true,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        ["transform-decorators-legacy"]
    ]
}